{
        "verbose": true,
        "dir_input": "/var/tmp/maab/kronos-core/input",
        "dir_output": "/var/tmp/maab/kronos-core/output",
        "kpf_files": ["ecmwf_accounting_test.kpf", 
                      "ecmwf_darshan.kpf", 
                      "ecmwf_ipm.kpf", 
                      "ecmwf_stdout.kpf"],
        "ksf_filename": "schedule_ecmwf.ksf",
        "model": {
                "fill_in": {
                        "user_functions": [{
                                "type": "step",
                                "name": "step-1",
                                "x_step": 0.5
                        }, {
                                "type": "custom",
                                "name": "custom-1",
                                "x_values": [0, 0.1, 0.2, 0.3, 0.5, 0.8, 0.9, 1.0],
                                "y_values": [1.0, 0.5, 0.0, 0.0, 0.0, 0.0, 0.5, 1.0]
                        }],
                        "operations": [{
                                "type": "fill_missing_entries",
                                "apply_to": ["operational-ipm"],
                                "priority": 0,
                                "metrics": {
                                        "kb_collective": [100, 1000],
                                        "n_collective": [10, 1000],
                                        "kb_write": {
                                                "function": "step-1",
                                                "scaling": 1000.0
                                        },
                                        "n_pairwise": {
                                                "function": "custom-1",
                                                "scaling": 1000.0
                                        },
                                        "n_write": [10, 1000],
                                        "n_read": [10, 1000],
                                        "kb_read": [100, 1000],
                                        "flops": [1e9, 1e10],
                                        "kb_pairwise": [100, 1000]
                                }
                        }, {
                                "type": "match_by_keyword",
                                "priority": 10,
                                "keywords": ["label"],
                                "similarity_threshold": 1.0,
                                "source_workloads": ["operational-darshan"],
                                "apply_to": ["operational-ipm"]
                        }]
                },
                "classification": {
                        "operations": [{
                                "type": "split",
                                "apply_to": "operational-ipm",
                                "create_workload": "HR_other",
                                "split_by": "label",
                                "keywords_in": ["o/"],
                                "keywords_out": []
                        }, {
                                "type": "split",
                                "apply_to": "operational-ipm",
                                "create_workload": "ENS_data_assimilation",
                                "split_by": "label",
                                "keywords_in": ["eda/"],
                                "keywords_out": []
                        }, {
                                "type": "split",
                                "apply_to": "operational-ipm",
                                "create_workload": "ENS_other",
                                "split_by": "label",
                                "keywords_in": ["mc/"],
                                "keywords_out": []
                        }, {
                                "type": "split",
                                "apply_to": "operational-ipm",
                                "create_workload": "law",
                                "split_by": "label",
                                "keywords_in": ["law/"],
                                "keywords_out": []
                        }, {
                                "type": "split",
                                "apply_to": "operational-ipm",
                                "create_workload": "Other",
                                "split_by": "label",
                                "keywords_in": [],
                                "keywords_out": ["o/", "eda/", "mc/", "lag/"]
                        }, {
                                "type": "split",
                                "apply_to": "operational-ipm",
                                "create_workload": "HR_model",
                                "split_by": "label",
                                "keywords_in": ["o/", "main", "fc", "model"],
                                "keywords_out": []

                        }, {
                                "type": "split",
                                "apply_to": "operational-ipm",
                                "create_workload": "ENS_model",
                                "split_by": "label",
                                "keywords_in": ["mc/", "main", "fc", "model"],
                                "keywords_out": []
                        }],
                        "clustering": {
                                "apply_to": [
                                        "HR_other",
                                        "ENS_data_assimilation",
                                        "ENS_other",
                                        "law",
                                        "Other",
                                        "HR_model",
                                        "ENS_model"
                                ],
                                "type": "Kmeans",
                                "ok_if_low_rank": true,
                                "user_does_not_check": true,
                                "rseed": 0,
                                "max_iter": 100,
                                "max_num_clusters": 20,
                                "delta_num_clusters": 1,
                                "num_timesignal_bins": 5
                        }
                },
                "generator": {
                        "type": "match_job_pdf_exact",
                        "n_bins_for_pdf": 20,
                        "random_seed": 0,
                        "tuning_factors": {
                                "kb_collective": 1e1,
                                "n_collective": 1e-1,
                                "kb_write": 1e-4,
                                "n_pairwise": 1e-1,
                                "n_write": 1e-4,
                                "n_read": 1e-3,
                                "kb_read": 1e-3,
                                "flops": 1e1,
                                "kb_pairwise": 10e-0
                        },
                        "submit_rate_factor": 4.0,
                        "synthapp_n_cpu": 2,
                        "synthapp_n_nodes": 1,
                        "synthapp_n_frames": 10,
                        "total_submit_interval": 300
                }
        }
}

