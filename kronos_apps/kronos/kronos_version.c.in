#include "kronos/kronos_version.h"

const char * kronos_version()     { return KRONOS_VERSION; }
const char * kronos_version_str() { return KRONOS_VERSION; }

unsigned int kronos_version_int()
{
  return 10000*KRONOS_VERSION_MAJOR + 100*KRONOS_VERSION_MINOR + 1*KRONOS_VERSION_PATCH;
}

const char * kronos_git_sha1() { return "@kronos_GIT_SHA1@"; }

