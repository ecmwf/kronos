#!/usr/bin/env python2.7

import os
import sys

sys.path.append(os.path.dirname(os.path.dirname(__file__)))

from kronos.executor.global_config import global_config

if len(sys.argv) != 2:
    print "Print values in the global kronos configuration. Helpful for bootstrapping them"
    print "into the CMake configuration"
    print ""
    print "Usage: "
    print "    kronos_config_enquire <parameter>"
    sys.exit(-1)

print global_config.get(sys.argv[1], "[Unknown]")
